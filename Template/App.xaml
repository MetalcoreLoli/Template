<Application x:Class="Template.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Template"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Views/ResourceDictionaries/FontDictionary.xaml"/>
                <ResourceDictionary Source="Views/ResourceDictionaries/BaseDictionary.xaml"/>
                <ResourceDictionary Source="Views/ResourceDictionaries/ButtonsDictionary.xaml"/>
                <ResourceDictionary Source="Views/ResourceDictionaries/TextDicionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary> 
    </Application.Resources>
</Application>
